{
    "name": "perfect-oblivion/laravel-microsoft-graph",
    "description": "A Laravel Microsoft Graph API (Office365) package",
    "version": "3.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@daveismyname.com",
            "homepage": "https://daveismyname.blog"
        },
        {
            "name": "Clayton Stone",
            "email": "clay@perfectoblivion.com",
            "homepage": "https://perfectoblivion.com"
        }
    ],
    "homepage": "https://github.com/perfect-oblivion/laravel-microsoft-graph",
    "keywords": ["Laravel", "MsGraph", "Graph", "Microsoft Graph", "Office365"],
    "require": {
        "illuminate/support": "5.8.*",
        "league/oauth2-client": "^2.0",
        "guzzlehttp/guzzle": "^6.0"
